export interface Post {
    title: string;
    slug: { current: string };
    publishedAt: string;
    excerpt: string;
    video?: string;
    image?: {
        asset: {
            _ref: string;
            _type: string;
        };
        alt?: string;
    };
    body: Array<any>;
    categories?: Array<{ _ref: string; _type: string }>;
    tags?: Array<Tag>;
    _id: string;
}
export interface Tag {
    name: string;
    slug: { current: string };
    _id: string;
}